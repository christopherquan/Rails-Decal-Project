<h3>Welcome to League Manager</h3>
<td><%= button_to "Logout", user_path(id: "sign_out"), :class => "button small", :method => :delete %></td>
<p><%= link_to 'Users:', users_path %> <%= User.count %> registered</p>
<td><%= button_to "View Clubs", clubs_path, :class => "button small", :method => :get %></td>
<div><%= button_to "Create Club", new_clubs_path, :class => "button medium", :method => :get %></div>
<td><%= Club.find(@user.club_id).name %></td>
